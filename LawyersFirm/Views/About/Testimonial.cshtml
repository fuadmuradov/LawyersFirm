                    <div class="col-lg-6">
                        <div class="testimonial-wrapper">
                            <div class="testimonial-content">
                                <h6>@item.Description</h6>
                            </div>
                            <div class="icon-box-wrapper">
                                <div class="icon-box-icon testimonial-client"><img src="~/assets/images/testimonials/@item.Image" alt=""></div>
                                <div class="icon-box-content">
                                    <div class="client-name">@item.Fullname</div>
                                    <p class="client-company">@item.Componyname</p>
                                </div>
                            </div>
                        </div>
                    </div>
                }


