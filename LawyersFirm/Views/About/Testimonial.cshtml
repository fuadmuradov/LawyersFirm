@model List<Testimonial>

		<!-- TOP IMAGE -->
<div class="page-head " style="background-image:url(../assets/images/top-bkgs/about.jpg);">
    <div class="inner-desc">
        <div class="container">
            <h1 class="page-title">Testimonials</h1>
            <span class="post-subtitle page-subtitle">Dictum ultricies magna ultricies</span>
        </div>
