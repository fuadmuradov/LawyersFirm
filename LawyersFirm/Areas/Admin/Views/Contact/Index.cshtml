@model Setting

<div class="body-content">
    <div class="row">

        <div class="col-md-12 col-lg-12">
            <div class="card center-block mb-5" style="width:50rem;">
                <div class="card-body">
                    <form method="post">
                        <div asp-validation-summary="ModelOnly"></div>
                        <div class="form-group">
                            <label asp-for="Phone" class="font-weight-600">Phone Number</label>
                            <input asp-for="Phone" class="form-control" id="exampleFormControlInput1">
                            <span asp-validation-for="Phone"></span>
                        </div>
                        <div class="form-group">
                            <label asp-for="Email" class="font-weight-600">Email</label>
                            <input asp-for="Email" class="form-control" id="exampleFormControlInput1">
                            <span asp-validation-for="Email"></span>
                        </div>
                        <div class="form-group">
                            <label asp-for="Address" class="font-weight-600">Address</label>
                            <input asp-for="Address" class="form-control" id="exampleFormControlInput1">
                            <span asp-validation-for="Address"></span>
                        </div>
                        <div class="form-group">
                            <label asp-for="Facebook" class="font-weight-600">Facebook URL</label>
                            <input asp-for="Facebook" class="form-control" id="exampleFormControlInput1">
                            <span asp-validation-for="Facebook"></span>
                        </div>
                        <div class="form-group">
                            <label asp-for="Twitter" class="font-weight-600">Twitter URL</label>
                            <input asp-for="Twitter" class="form-control" id="exampleFormControlInput1">
                            <span asp-validation-for="Twitter"></span>
                        </div>
                        <div class="form-group">
                            <label asp-for="Linkedin" class="font-weight-600">Linkedin URL</label>
                            <input asp-for="Linkedin" class="form-control" id="exampleFormControlInput1">
                            <span asp-validation-for="Linkedin"></span>
                        </div>
                        <div class="form-group">
                            <label asp-for="@Model.Photo" class="font-weight-600">Photo</label>
                            <input asp-for="@Model.Photo" class="form-control" id="exampleFormControlInput1">
                            <span asp-validation-for="@Model.Photo"></span>
                        </div>
                        <div class="form-group">
                            <label asp-for="Id" class="font-weight-600">ID</label>
                            <input asp-for="Id" class="form-control" id="exampleFormControlInput1">
                            <span asp-validation-for="Id"></span>
                        </div>
                        <div style="width:210px; height:68px; background-color:darkgrey">
                            <img src="~/assets/images/@Model.Logo" alt="Logo" width="200" height="65">
                        </div>
                        <button type="submit" class="btn btn-success mt-3" onclick="return confirm('Do you Want Update Setting Information')">Update</button>
                    </form>
                </div>
            </div>


       


        </div>
    </div>
</div>
